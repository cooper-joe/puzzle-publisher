        {
            "script": "menu-cmd-other.js",
            "handler": "showChangeLog",
            "shortcut": "",
            "name": "Show Change Log",
            "identifier": "showChangeLog"
        }
    ],
    "menu": {
        "title": "‚úàÔ∏è Puzzle Publisher",
        "items": [
